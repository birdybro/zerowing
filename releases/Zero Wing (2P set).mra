        <part crc="351ba71a" name="o15-09.rom" map="10" />
        <part crc="13764e95" name="o15-10.rom" map="01" />
    </interleave>
     
    <!-- gfx1       0x050000-0x0CFFFF 512K -->
    <interleave output="32">    
        <part crc="4e5dd246" name="o15-05.rom" map="0001"/>
